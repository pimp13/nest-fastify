model WorkSpace {
    id        String    @id @default(uuid())
    name      String
    slug      String    @unique
    ownerId   String
    owner     User      @relation(fields: [ownerId], references: [id])
    imageUrl  String?
    createdAt DateTime  @default(now())
    updatedAt DateTime  @updatedAt
    projects  Project[]
    members   Member[]
}
